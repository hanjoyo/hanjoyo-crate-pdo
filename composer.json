{
    "name": "hanjoyo/hanjoyo-crate-pdo",
    "repositories": [
    {
        "type": "package",
        "package": {
            "name": "hanjoyo-crate-pdo",
            "version": "1.0",
            "source": {
                "url": "https://github.com/hanjoyo/hanjoyo-crate-pdo.git",
                "type": "git",
                "reference": "master"
            }
        }
    }],
    "keywords": ["database", "pdo", "crate"],
    "require": {
        "php":     "^7.2",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-0": {
            "Crate\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpstan/phpstan": "^0.9.2",
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "autoload-dev": {
        "psr-0": {
            "CrateTest\\PDO\\": "test",
            "CrateIntegrationTest\\PDO\\": "test"
        }
}